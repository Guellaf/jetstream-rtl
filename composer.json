{
    "name": "alifattahi/jetstream-rtl",
    "description": "RTL Tailwind scaffolding for the Laravel framework.",
    "keywords": ["laravel", "tailwind", "auth","rtl"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/jetstream/issues",
        "source": "https://github.com/laravel/jetstream"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
		{
            "name": "Ali Fattahi",
            "email": "ali.robocup@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.0"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.2.4",
        "laravel/sanctum": "^2.6",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Jetstream\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
